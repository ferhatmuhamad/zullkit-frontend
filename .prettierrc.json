{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "bracketSameLine": true,
  "vueIndentScriptAndStyle": true,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.vue",
      "options": {
        "bracketSameLine": true
      }
    }
  ]
}
